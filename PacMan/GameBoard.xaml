<UserControl x:Class="PacMan.GameBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:l="clr-namespace:PacMan"
             mc:Ignorable="d" Height="546" Width="1134">
    <Canvas x:Name="gameBoard" Width="1134" Height="546" VerticalAlignment="Top" HorizontalAlignment="Left">
        <Canvas.Background>
            <ImageBrush ImageSource="Images/CIS209Board.png" Stretch="None"/>
        </Canvas.Background>
        <l:Pacman x:Name="PM" Canvas.Left="504" Canvas.Top="378"></l:Pacman>
    </Canvas>
</UserControl>
